<h1> Listing Restaurants </h1>
<table border = 1> 
  <tr>
    <th>Name</th>
    <th> Address  </th>
    <th> Phone Numbers </th>
    <th>Timings</th>
    <th>Ratings</th>
  </tr>
<% index = 0 %>
<% @result.each do |res| %>
	<tr>
		<td> <%= res.rest_name %> </td>		
		<td> <%= res.address %> </td>
		<td> <%= res.phone_number %> </td>
		<td> <%= res.timings %> </td>
		<td> <%= @rating_result[index] %> </td> 
		<% index = index+1 %>
	</tr>
	
<% end %>
<%# link_to 'Search',action:'dish/find',method: :get %> 
</table>
